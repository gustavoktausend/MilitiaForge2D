;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MilitiaForge2D"
config/description="A modular component-based framework for rapid 2D game development in Godot 4.x"
config/version="0.1.0"
run/main_scene="res://examples/space_shooter/scenes/main_menu.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://examples/space_shooter/scripts/audio_manager.gd"
PlayerData="*res://examples/space_shooter/scripts/player_data.gd"
EntityPoolManager="*res://examples/space_shooter/scripts/entity_pool_manager.gd"
SceneManager="*res://addons/scene_manager/scene_manager.tscn"
Scenes="*res://addons/scene_manager/scenes.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="MilitiaForge2D"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/scene_manager/plugin.cfg")

